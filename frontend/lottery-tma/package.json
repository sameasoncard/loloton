{
  "name": "lottery-tma",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "homepage": "https://sameasoncard.github.io/loloton/",
  "scripts": {
    "dev": "vite --host",
    "build": "rm -rf ../../docs/* && tsc --noEmit && vite build && cp -r ./dist/* ../../docs/ && git add ../../docs/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",

    "deploy": "gh-pages -d dist",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "predeploy": "pnpm run build"
  },
  "dependencies": {
    "@telegram-apps/telegram-ui": "^2.1.3",
    "@tonconnect/ui-react": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",

    "@tma.js/react-router-integration": "^4.0.0",
    "@tma.js/sdk-react": "^2.2.1",
    "eruda": "^3.0.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",

    "@eslint/js": "^9.2.0",
    "@types/node": "^20.12.11",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "eslint-plugin-react": "^7.34.1",
    "gh-pages": "^6.1.1",
    "globals": "^15.2.0",
    "typescript-eslint": "^7.8.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
